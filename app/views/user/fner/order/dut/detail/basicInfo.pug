.border.border-dark.p-3
	.row.text-center
		.col-md-6
			h5.code 订单号: #{dut.code}
			.time.mt-2
				span 下单时间: 
				span #{moment(dut.dutAt).format('DD/MM/YYYY')}
		.col-md-6
			.dut.mt-2
				span 采购员:
				if dut.order
					- order = dut.order;
					span #{order.nome} [ #{order.code} ]
				else
					span 数据丢失
			.strmup 供应商: 
				if dut.strmup
					- strmup = dut.strmup;
					span #{strmup.nome}
				else
					span 未分配
			.status
				- let status = '';
				each sts in Conf.status
					- status = '';
					if sts.num == dut.status
						- status = sts.val;
						- break;
				span 状态: #{status}