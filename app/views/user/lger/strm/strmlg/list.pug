extends ../../../aalayout/layout

block content
	.container.mt-3
		nav(aria-label="breadcrumb")
			ol.breadcrumb
				li.breadcrumb-item: a(href="/") Home
				li.breadcrumb-item.active(aria-current="page") 物流公司列表
		.add.text-right
			a.btn.btn-warning(href="/lgStrmlgAdd") +添加物流公司
		.row.mt-3.text-center
			each strmlg in strmlgs
				-a;
				.col-md-3.card.p-3.mt-3
					a.btn.btn-info(href="/lgStrmlg/"+strmlg._id) #{strmlg.code}
					h5.mt-3 #{strmlg.nome}
